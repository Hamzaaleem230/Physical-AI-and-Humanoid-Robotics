{
  "version": 2,
  "builds": [
    {
      "src": "Physical-AI-and-Humanoid-Robotics-Book/package.json",
      "use": "@vercel/static-build",
      "config": {
        "installCommand": "npm install",
        "buildCommand": "npm run build"
      }
    },
    {
      "src": "backend/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "Physical-AI-and-Humanoid-Robotics-Book/$1"
    }
  ]
}