{
  "builds": [
    {
      "src": "Physical-AI-and-Humanoid-Robotics-Book/package.json",
      "use": "@vercel/static-build",
      "config": {
        "outputDirectory": "build"
      }
    },
    {
      "src": "backend/app/main.py",
      "use": "@vercel/python",
      "config": {
        "maxDuration": 15
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "Physical-AI-and-Humanoid-Robotics-Book/$1"
    }
  ]
}